<AnchorPane fx:id="background" onKeyReleased="#key" prefHeight="720.0" prefWidth="1080.0" styleClass="background" stylesheets="@select.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.Boxgame.Select">
